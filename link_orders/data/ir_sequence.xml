<odoo>
    <data>
        <record id="seq_mrp_bom_lot_number" model="ir.sequence">
            <field name="name">BOM Lot Number</field>
            <field name="code">mrp.bom.lot.number</field>
            <field name="prefix">LR-%(month)s%(y)s</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="use_date_range">True</field>
        </record>

        <record id="seq_jc_internal_picking" model="ir.sequence">
            <field name="name">JC Internal Transfer</field>
            <field name="code">stock.picking.internal.jc</field>
            <field name="prefix">JC-%(month)s%(y)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_sale_order_monthly" model="ir.sequence">
            <field name="name">Sales Order Monthly</field>
            <field name="code">sale.order.monthly</field>
            <field name="prefix">PI-%(month)s%(y)s</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="use_date_range">True</field>
        </record>

  

 
    </data>

</odoo>
